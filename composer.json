{
  "name": "laravel/laravel",
  "type": "project",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "require": {
    "php": "^7.4",
    "aws/aws-sdk-php": "~3.99.4",
    "blade-ui-kit/blade-icons": "^v0.3.4",
    "calebporzio/awesome-helpers": "^v2.4.5",
    "coconutcraig/laravel-postmark": "^v2.9.0",
    "fideloper/proxy": "^4.4.0",
    "fruitcake/laravel-cors": "^v2.0.1",
    "guzzlehttp/guzzle": "^6.5.5",
    "laravel/framework": "v7.9.2",
    "laravel/horizon": "^v4.3.3",
    "laravel/nova": "~3.0",
    "laravel/sanctum": "^v2.4.2",
    "laravel/spark-aurelius": "~10.0",
    "laravel/telescope": "^3.5",
    "laravel/tinker": "v2.4.2",
    "laravel/ui": "^v2.1.0",
    "livewire/livewire": "^v1.3.3",
    "predis/predis": "^v1.1.3",
    "spatie/laravel-honeypot": "^2.2.0",
    "spatie/laravel-mailcoach": "^2.0.0",
    "spatie/laravel-mailcoach-mailgun-feedback": "^2.0",
    "spatie/laravel-mailcoach-postmark-feedback": "^2.0",
    "spatie/laravel-mailcoach-sendgrid-feedback": "^2.0",
    "spatie/laravel-mailcoach-ses-feedback": "^2.0",
    "wildbit/swiftmailer-postmark": "^3.1.0"
  },
  "require-dev": {
    "barryvdh/laravel-debugbar": "v3.4.1",
    "facade/ignition": "^2.3.6",
    "fzaninotto/faker": "^v1.9.1",
    "laracasts/cypress": "^1.1",
    "mockery/mockery": "^1.4.2",
    "nunomaduro/collision": "^v4.2.0",
    "phpunit/phpunit": "9.3.x-dev"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    },
    "classmap": [
      "database/seeds",
      "database/factories"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ],
    "post-update-cmd": [
      "@php artisan vendor:publish --tag mailcoach-assets --force",
      "@php artisan horizon:publish --ansi",
      "@php artisan nova:publish"
    ]
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://satis.mailcoach.app"
    },
    {
      "type": "composer",
      "url": "https://spark-satis.laravel.com"
    },
    {
      "type": "composer",
      "url": "https://nova.laravel.com"
    }
  ]
}
