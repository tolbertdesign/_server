{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.4",
        "aws/aws-sdk-php": "~3.99.4",
        "barryvdh/laravel-translation-manager": "^v0.5.8",
        "blade-ui-kit/blade-icons": "^v0.3.4",
        "fideloper/proxy": "^4.4.0",
        "fruitcake/laravel-cors": "^v2.0.1",
        "guzzlehttp/guzzle": "^6.5.5",
        "laravel/framework": "v7.9.2",
        "laravel/horizon": "^v4.3.3",
        "laravel/sanctum": "^v2.4.2",
        "laravel/spark-aurelius": "~10.0",
        "laravel/tinker": "^v2.4.1",
        "laravel/ui": "^v2.1.0",
        "livewire/livewire": "^v1.3.2",
        "predis/predis": "dev-master",
        "spatie/laravel-mailcoach": "^2.0.0",
        "spatie/laravel-mailcoach-ses-feedback": "^2.0",
        "spatie/laravel-translation-loader": "^2.6"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^v3.3.3",
        "facade/ignition": "^2.3.5",
        "fzaninotto/faker": "^v1.9.1",
        "mockery/mockery": "^1.4.1",
        "nunomaduro/collision": "^v4.2.0",
        "phpunit/phpunit": "9.3.x-dev"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag mailcoach-assets --force",
            "@php artisan horizon:publish --ansi"
        ]
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://satis.mailcoach.app"
        },
        {
            "type": "composer",
            "url": "https://spark-satis.laravel.com"
        }
    ]
}
